GVL1.Pwi_DigitalInputs[7].12	:= NOT w_im8IOstatus.12	AND NOT GVL1.hwo_digitalAlarms[12].7	;	(*	DI:125	AMS REDUNDANCY MODULE ALARM	*)
GVL1.Pwi_DigitalInputs[7].13	:= NOT w_im8IOstatus.13	AND NOT GVL1.hwo_digitalAlarms[12].7	;	(*	DI:126	AMS MAIN SUPPLY FAIL	*)
GVL1.Pwi_DigitalInputs[7].14	:= NOT w_im8IOstatus.14	AND NOT GVL1.hwo_digitalAlarms[12].7	;	(*	DI:127	AMS EMG SUPPLY FAIL	*)
