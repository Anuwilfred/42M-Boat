//Digital input module1  MP- S3306: 16CH							
GVL1.Pwi_DigitalInputs[8].0	:= NOT w_im9IOstatus.0	AND NOT GVL1.hwo_digitalAlarms[12].8	;	(*	DI:129	SPARE	*)
