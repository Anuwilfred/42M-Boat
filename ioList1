//Digital output module1 - S3303E: 16Ch	
poweron_ton(IN:=TRUE , PT:=T#10S , Q=> , ET=> );
w_om1IOStatus.0	 := comAlarm3.LDo_Accoustic1 AND poweron_ton.Q AND Delay_ton.Q;	(*	BUZZER WITH IND CHIEF ENG ROOM	*)	
Delay_ton(IN:=comAlarm.LDo_Accoustic1 , PT:=T#180S , Q=> , ET=> );
w_om1IOStatus.1	 := comAlarm1.LDo_Accoustic1 AND poweron_ton.Q;	(*	BUZZER WITH IND WHEEL HOUSE	*)
w_om1IOStatus.2	 := comAlarm2.LDo_Accoustic1 AND poweron_ton.Q;	(*	BUZZER WITH IND MESS ROOM	*)
w_om1IOStatus.3	 := comAlarm.LDo_Accoustic1 AND poweron_ton.Q;	(*	VISUAL & AUDIO ALARM BUZZER ENG: ROOM	*)
w_om1IOStatus.4	 := comAlarm.LDo_Accoustic1 AND poweron_ton.Q;	(*	VISUAL & AUDIO ALARM BUZZER GEN: ROOM	*)
w_om1IOStatus.5	 := comAlarm.LDo_Accoustic1 AND poweron_ton.Q;	(*	VISUAL & AUDIO ALARM BUZZER PUMP: ROOM	*)

(*	GREY WATER TANK LOW AUTO STOP GREY WATER PUMP PUMP	*)
w_om1IOStatus.6	 := GVL1.pwo_digitalOutputs[1].6	;	
IF GVL1.hwo_analogAlm[11].1 AND NOT GVL1.hwi_override.0 
THEN GVL1.pwo_digitalOutputs[1].6:= 1; ELSE GVL1.pwo_digitalOutputs[1].6:=0;	END_IF

(*	FUEL TANK LOW AUTO STOP FO PUMP	*)
w_om1IOStatus.7	 := GVL1.pwo_digitalOutputs[1].7	;	
IF (GVL1.hwo_analogAlm[0].2 AND NOT GVL1.hwi_override.1)OR
( GVL1.hwo_analogAlm[1].2 AND NOT GVL1.hwi_override.2)OR
(GVL1.hwo_analogAlm[2].2 AND NOT GVL1.hwi_override.3)
THEN GVL1.pwo_digitalOutputs[1].7:= 1; ELSE GVL1.pwo_digitalOutputs[1].7:=0;	END_IF

(*	BT LOW OIL LEVEL / HIGH TEMP OUTPUT = engine sd	*)
w_om1IOStatus.8:= GVL1.pwo_digitalOutputs[1].8	;
ton_enginesd(IN:= GVL1.hwo_digitalAlarms[3].8 AND NOT overide_out, PT:= T#30S , Q=> , ET=> GVL1.enginesd_cd);
IF GVL1.hwo_digitalAlarms[3].8 AND (GVL1.hxi_enginesd_overide OR overide_out) THEN overide_out:=1 ;ELSE overide_out:=0; END_IF
IF (GVL1.hwo_digitalAlarms[3].8 OR GVL1.hwo_digitalAlarms[3].9) AND 
(GVL1.hxi_enginesd_accept OR GVL1.pwo_digitalOutputs[1].8 OR ton_enginesd.Q)
THEN GVL1.pwo_digitalOutputs[1].8:=1; ELSE GVL1.pwo_digitalOutputs[1].8:=0; END_IF

(*	SLOPE TK LOW AUTO STOP SLUDGE PUMP *)
w_om1IOStatus.9	 := GVL1.pwo_digitalOutputs[1].9	;	
IF GVL1.hwo_analogAlm[10].1 AND NOT GVL1.hwi_override.4 
THEN GVL1.pwo_digitalOutputs[1].9:= 1; ELSE GVL1.pwo_digitalOutputs[1].9:=0;	END_IF

(*	FODS HIGH AUTO STOP-1 TO FO PURIFIER 	*)
w_om1IOStatus.10	 := GVL1.pwo_digitalOutputs[1].10	;	
IF (GVL1.hwo_analogAlm[8].2 AND NOT GVL1.hwi_override.5) OR (GVL1.hwo_analogAlm[9].2 AND NOT GVL1.hwi_override.6)
THEN GVL1.pwo_digitalOutputs[1].10:= 1; ELSE GVL1.pwo_digitalOutputs[1].10:=0;	END_IF

(*	FODS HIGH AUTO STOP-2 TO FO PURIFIER 	*)
w_om1IOStatus.11:= GVL1.pwo_digitalOutputs[1].11	;	
IF (GVL1.hwo_analogAlm[8].2 AND NOT GVL1.hwi_override.5) OR (GVL1.hwo_analogAlm[9].2 AND NOT GVL1.hwi_override.6)
THEN GVL1.pwo_digitalOutputs[1].11:= 1; ELSE GVL1.pwo_digitalOutputs[1].11:=0;	END_IF

w_om1IOStatus.12 := GVL1.pwo_digitalOutputs[1].12	;	(*	SPARE	*)
w_om1IOStatus.13 := GVL1.pwo_digitalOutputs[1].13	;	(*	SPARE	*)
w_om1IOStatus.14 := GVL1.pwo_digitalOutputs[1].14	;	(*	SPARE	*)
w_om1IOStatus.15 := GVL1.pwo_digitalOutputs[1].15	;	(*	SPARE	*)



